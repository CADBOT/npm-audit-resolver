{
  "name": "npm-audit-resolver",
  "version": "1.0.1-0",
  "description": "Aids humans and automation in managing npm audit results",
  "bin": {
    "resolve-audit": "index.js",
    "check-audit": "check.js"
  },
  "scripts": {
    "add-vulns": "npm i -D base64url@2.0.0 lodash@2.0.0",
    "rm-vulns": "npm rm -D base64url lodash",
    "test": "npm run add-vulns && node index.js && node check.js && npm run rm-vulns"
  },
  "keywords": [
    "npm",
    "audit",
    "security",
    "dependencies",
    "nsp",
    "check"
  ],
  "author": "naugtur <naugtur@gmail.com>",
  "license": "Apache 2.0",
  "dependencies": {
    "concat-stream": "^1.6.2",
    "flatten": "^1.0.2",
    "promptly": "^3.0.3",
    "spawn-shell": "^2.0.1"
  },
  "devDependencies": {}
}
